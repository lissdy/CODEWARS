   "source": [
    "def fibonacci(num)\n",
    "  if num == 0 || num == 1\n",
    "    1\n",
    "  else\n",
    "    fibonacci(num-1) + fibonacci(num-2)\n",
    "  end\n",
    "end"
   ]
